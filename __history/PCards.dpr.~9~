program PCards;

{$APPTYPE CONSOLE}
{$R *.res}

uses
  System.SysUtils,
  UCard in 'UCard.pas',
  UPack in 'UPack.pas',
  UHand in 'UHand.pas',
  UExceptions in 'UExceptions.pas',
  UGame in 'UGame.pas';

var
  plar1, plar2: integer;

begin
  init;
  for I := 0 to 10001 do
  begin
    if play = 1 then
      inc(plar1)
    else
      inc(plar2);
  end;

  writeln(plar1);
  writeln(plar2);
  readln;

end.
